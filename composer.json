  "keywords": [
    "gravatar",
    "integration",
    "twig"
  ],
  "type": "library",
  "homepage": "http://nicolas-gille.fr",
  "license": "MIT",
  "authors": [
    {
      "name": "Nicolas GILLE",
      "email": "nic.gille@gmail.com",
      "homepage": "http://nicolas-gille.fr",
      "role": "Developer"
    }
  ],
  "support": {
    "email": "nic.gille@gmail.com",
    "issues": "https://github.com/Kero76/gravatar-lib/issues",
    "source": "https://github.com/Kero76/gravatar-lib"
  },
  "require": {
    "php": ">=7.0.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.3"
  },
  "suggest": {
    "twig/twig": ">=1.4.0"
  },
  "autoload": {
    "psr-4": {
